<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Image.
    -->
    <changeSet id="20230114162748-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="extended_user_id"
                                 baseTableName="image"
                                 constraintName="fk_image__extended_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="extended_user"/>

        <addForeignKeyConstraint baseColumnNames="like_image_id"
                                 baseTableName="image"
                                 constraintName="fk_image__like_image_id"
                                 referencedColumnNames="id"
                                 referencedTableName="like_image"/>
    </changeSet>
</databaseChangeLog>
