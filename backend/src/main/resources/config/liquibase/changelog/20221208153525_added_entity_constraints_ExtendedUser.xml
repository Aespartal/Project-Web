<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity ExtendedUser.
    -->
    <changeSet id="20221208153525-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="id"
                                 baseTableName="extended_user"
                                 constraintName="fk_extended_user__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="like_image_id"
                                 baseTableName="extended_user"
                                 constraintName="fk_extended_user__like_image_id"
                                 referencedColumnNames="id"
                                 referencedTableName="like_image"/>

        <addForeignKeyConstraint baseColumnNames="like_commentary_id"
                                 baseTableName="extended_user"
                                 constraintName="fk_extended_user__like_commentary_id"
                                 referencedColumnNames="id"
                                 referencedTableName="like_commentary"/>
    </changeSet>
</databaseChangeLog>
